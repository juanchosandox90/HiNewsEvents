<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/form"
    app:startDestination="@id/home"
    tools:ignore="UnusedNavigation">

    <fragment
            android:id="@+id/home"
            android:name="com.huawei.hinewsevents.ui.home.HomeFragment"
            android:label="@string/title_home"
            tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home_to_home_details"
            app:destination="@id/home_detail"
            app:popUpTo="@+id/home"
            app:launchSingleTop="true"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/home_detail"
        android:name="com.huawei.hinewsevents.ui.home.HomeDetailFragment"
        android:label="Registered"
        tools:layout="@layout/fragment_home_detail"/>
</navigation>
